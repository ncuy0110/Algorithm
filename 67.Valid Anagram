class Solution {
    public boolean isAnagram(String s, String t) {
        int[] a = new int[123];
        for(int i=0; i<s.length();i++){
            a[(int)s.charAt(i)]++;
        }
        for(int i=0; i<t.length(); i++){
            a[(int)t.charAt(i)]--;
        }
        for(int i=97; i<123; i++) {
            if(a[i]!=0) return false;
        }
        return true;
    }
